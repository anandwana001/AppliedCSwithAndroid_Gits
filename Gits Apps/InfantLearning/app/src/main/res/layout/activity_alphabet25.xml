<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.infantlearning.infantlearning.Alphabet25">

    <Button
        android:id="@+id/button151"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp"
        android:text="Previous"
        android:textSize="20dp"/>

    <Button
        android:id="@+id/button152"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button151"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="Next"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/textView143"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button152"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/button152"
        android:text="Y y"
        android:textSize="100dp" />

    <TextView
        android:id="@+id/textView144"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button152"
        android:layout_alignEnd="@+id/button152"
        android:layout_alignRight="@+id/button152"
        android:layout_marginBottom="71dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:text="Yak"
        android:textSize="50dp" />

    <ImageView
        android:id="@+id/imageView75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button152"
        android:layout_alignRight="@+id/button152"
        android:layout_centerVertical="true"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        app:srcCompat="@drawable/yak" />
</RelativeLayout>
