        android:textSize="150dp" />

    <TextView
        android:id="@+id/textView87"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView86"
        android:layout_alignStart="@+id/textView86"
        android:layout_below="@+id/textView86"
        android:layout_marginTop="84dp"
        android:text="ओखली"
        android:textSize="50dp" />

    <ImageView
        android:id="@+id/imageView45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/textView86"
        android:layout_toStartOf="@+id/textView86"
        app:srcCompat="@drawable/okhli" />
</RelativeLayout>
