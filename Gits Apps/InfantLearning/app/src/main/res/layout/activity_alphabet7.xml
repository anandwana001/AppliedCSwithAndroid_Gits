<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.infantlearning.infantlearning.Alphabet7">

    <Button
        android:id="@+id/button115"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="13dp"
        android:layout_marginLeft="21dp"
        android:text="Previous"
        android:textSize="20dp"/>

    <Button
        android:id="@+id/button116"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/button115"
        android:layout_marginRight="16dp"
        android:text="Next"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView108"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button116"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/button116"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="21dp"
        android:text="G g"
        android:textSize="100dp" />

    <TextView
        android:id="@+id/textView109"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button116"
        android:layout_alignEnd="@+id/imageView56"
        android:layout_alignRight="@+id/imageView56"
        android:layout_marginBottom="62dp"
        android:text="Gun"
        android:textSize="50dp" />

    <ImageView
        android:id="@+id/imageView56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button115"
        android:layout_alignEnd="@+id/textView108"
        android:layout_alignRight="@+id/textView108"
        android:layout_marginEnd="54dp"
        android:layout_marginRight="54dp"
        app:srcCompat="@drawable/gun" />
</RelativeLayout>
