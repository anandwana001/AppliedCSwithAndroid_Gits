<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.infantlearning.infantlearning.Alphabet19">

    <Button
        android:id="@+id/button139"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="11dp"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        android:text="Previous"
        android:textSize="20dp"/>

    <Button
        android:id="@+id/button140"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/button139"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:text="Next"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/textView131"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button140"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/button140"
        android:text="S s"
        android:textSize="100dp" />

    <TextView
        android:id="@+id/textView132"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button139"
        android:layout_alignLeft="@+id/button139"
        android:layout_alignStart="@+id/button139"
        android:layout_marginBottom="66dp"
        android:text="Sun"
        android:textSize="50dp" />

    <ImageView
        android:id="@+id/imageView68"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button139"
        android:layout_alignLeft="@+id/button139"
        android:layout_alignStart="@+id/button139"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        app:srcCompat="@drawable/sun" />
</RelativeLayout>
