<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.infantlearning.infantlearning.Alphabet8">

    <Button
        android:id="@+id/button117"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="13dp"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp"
        android:text="Previous"
        android:textSize="20dp"/>

    <Button
        android:id="@+id/button118"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button117"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="Next"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/textView107"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button118"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/button118"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:text="H h"
        android:textSize="100dp" />

    <TextView
        android:id="@+id/textView110"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button118"
        android:layout_alignEnd="@+id/textView107"
        android:layout_alignRight="@+id/textView107"
        android:layout_marginBottom="59dp"
        android:text="Horse"
        android:textSize="50dp" />

    <ImageView
        android:id="@+id/imageView57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button117"
        android:layout_alignStart="@+id/button117"
        android:layout_below="@+id/textView107"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp"
        app:srcCompat="@drawable/horse" />
</RelativeLayout>
