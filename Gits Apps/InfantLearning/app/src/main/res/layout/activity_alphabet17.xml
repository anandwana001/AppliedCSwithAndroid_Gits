<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.infantlearning.infantlearning.Alphabet17">

    <Button
        android:id="@+id/button135"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="13dp"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:text="Previous"
        android:textSize="20dp"/>

    <Button
        android:id="@+id/button136"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/button135"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="Next"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/textView127"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="26dp"
        android:layout_toEndOf="@+id/imageView66"
        android:layout_toRightOf="@+id/imageView66"
        android:text="Q q"
        android:textSize="100dp" />

    <TextView
        android:id="@+id/textView128"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView66"
        android:layout_marginTop="20dp"
        android:layout_toEndOf="@+id/imageView66"
        android:layout_toRightOf="@+id/imageView66"
        android:text="Queen"
        android:textSize="50dp" />

    <ImageView
        android:id="@+id/imageView66"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/button135"
        android:layout_alignStart="@+id/button135"
        android:layout_alignTop="@+id/textView127"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="114dp"
        app:srcCompat="@drawable/queen" />
</RelativeLayout>
